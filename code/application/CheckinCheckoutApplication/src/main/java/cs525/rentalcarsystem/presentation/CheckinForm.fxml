<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="530.0" prefWidth="781.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="cs525.rentalcarsystem.controller.CheckinFormController">
	<center>
		<HBox prefHeight="273.0" prefWidth="592.0" BorderPane.alignment="CENTER">
			<children>
				<HBox prefHeight="273.0" prefWidth="525.0">
					<children>
						<VBox prefHeight="522.0" prefWidth="147.0">
							<children>
								<Label alignment="BOTTOM_RIGHT" prefHeight="39.0"
									prefWidth="118.0" text="CustomerId:">
									<VBox.margin>
										<Insets top="10.0" />
									</VBox.margin>
								</Label>
								<Label prefHeight="62.0" prefWidth="119.0"
									text="             Products:">
									<VBox.margin>
										<Insets top="25.0" />
									</VBox.margin>
								</Label>
							</children>
						</VBox>
						<VBox prefHeight="498.0" prefWidth="325.0">
							<children>
								<ComboBox fx:id="customerCombo" onAction="#onCustomerSelected"
									prefHeight="31.0" prefWidth="191.0" promptText="--Select Customer--">
									<VBox.margin>
										<Insets top="20.0" />
									</VBox.margin>
								</ComboBox>
								<TableView fx:id="productTable" prefHeight="200.0"
									prefWidth="200.0">
									<columns>
										<TableColumn fx:id="productTable" prefWidth="75.0"
											text="C1" />
										<TableColumn prefWidth="75.0" text="C2" />
									</columns>
									<VBox.margin>
										<Insets top="25.0" />
									</VBox.margin>
								</TableView>
								<Button fx:id="checkinBtn" defaultButton="true"
									mnemonicParsing="false" text="Checkin">
									<VBox.margin>
										<Insets top="40.0" />
									</VBox.margin>
								</Button>
							</children>
						</VBox>
					</children>
				</HBox>
			</children>
		</HBox>
	</center>
	<left>
		<VBox prefHeight="471.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
	</left>
	<right>
		<VBox prefHeight="273.0" prefWidth="178.0" BorderPane.alignment="CENTER" />
	</right>
	<top>
		<HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<children>
				<Label text="Checkin Form" underline="true">
					<HBox.margin>
						<Insets top="40.0" />
					</HBox.margin>
					<padding>
						<Insets left="250.0" />
					</padding>
					<font>
						<Font size="24.0" />
					</font>
				</Label>
			</children>
		</HBox>
	</top>
	<bottom>
		<HBox prefHeight="0.0" prefWidth="781.0" BorderPane.alignment="CENTER" />
	</bottom>
</BorderPane>
